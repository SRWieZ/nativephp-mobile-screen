{
    "name": "srwiez/nativephp-mobile-screen",
    "description": "Screen wake lock and brightness control for NativePHP Mobile apps",
    "type": "nativephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Eser Deniz",
            "email": "srwiez@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "nativephp/mobile": "^3.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "pestphp/pest": "^3.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "SRWieZ\\NativePHP\\Mobile\\Screen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SRWieZ\\NativePHP\\Mobile\\Screen\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SRWieZ\\NativePHP\\Mobile\\Screen\\ScreenServiceProvider"
            ],
            "aliases": {
                "Screen": "SRWieZ\\NativePHP\\Mobile\\Screen\\Facades\\Screen"
            }
        },
        "nativephp": {
            "manifest": "nativephp.json"
        }
    },
    "scripts": {
        "test": "pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
